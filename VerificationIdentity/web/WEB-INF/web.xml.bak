<?xml version="2.4" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Upload</servlet-name>
        <servlet-class>com.bank.servlet.action.Upload</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>OwnaccountAction</servlet-name>
        <servlet-class>com.bank.servlet.action.OwnaccountAction</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Transaction</servlet-name>
        <servlet-class>com.bank.servlet.action.Transaction</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Finger</servlet-name>
        <servlet-class>com.bank.servlet.action.Finger</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FingerVerification</servlet-name>
        <servlet-class>com.bank.servlet.action.FingerVerification</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Finger1</servlet-name>
        <servlet-class>com.bank.servlet.action.Finger1</servlet-class>
    </servlet>
	<servlet>
        <servlet-name>Transfer</servlet-name>
        <servlet-class>com.bank.servlet.action.Transfer</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Upload</servlet-name>
        <url-pattern>/Upload</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>OwnaccountAction</servlet-name>
        <url-pattern>/OwnaccountAction</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Transaction</servlet-name>
        <url-pattern>/Transaction</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Finger</servlet-name>
        <url-pattern>/Finger</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FingerVerification</servlet-name>
        <url-pattern>/FingerVerification</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Finger1</servlet-name>
        <url-pattern>/Finger1</url-pattern>
    </servlet-mapping>
	<servlet-mapping>
        <servlet-name>Transfer</servlet-name>
        <url-pattern>/Transfer</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            6000
        </session-timeout>
    </session-config>
</web-app>
